set terminal png transparent size <SIZE> crop
set output '<OUT>.png'
set xdata time
set timefmt "%Y-%m-%d_%H:%M:%S"
set xlabel " "
set title 'Systemlast Charon (Pi-hole)'
set ytics 
set y2tics 
set grid ytics
set ylabel "Load"
set y2label "Load"
set yrange [0:1.1]
set y2range [0:1.5]

#dblog sysstat_charon:load

plot "<IN>" using 1:2 axes x1y1 title '1 minute load average' ls l0 lw 1 with steps
